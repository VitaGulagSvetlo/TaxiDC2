<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              xmlns:local="clr-namespace:TaxiDC2" Title="ComboBox"
             xmlns:editors="clr-namespace:Syncfusion.Maui.Inputs;assembly=Syncfusion.Maui.Inputs"
             x:Class="TaxiDC2.ComboBoxFeatures">
    <ContentPage.BindingContext>
        <local:SocialMediaViewModels />
    </ContentPage.BindingContext>
    <editors:SfComboBox IsClearButtonVisible="True" IsEditable="True" IsDropdownButtonVisible="True" IsDropDownOpen="True" TextSearchMode="StartsWith" HeightRequest="50" VerticalOptions="Center" WidthRequest="300" ItemsSource="{Binding SocialMedias}" DisplayMemberPath="Name">
    </editors:SfComboBox>
</ContentPage>
